local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
   Name = "$ Cash hub",
   LoadingTitle = "$ Cash hub",
   LoadingSubtitle = "by cashjj ",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil, -- Create a custom folder for your hub/game
      FileName = "$ Cash hub"
   },
   Discord = {
      Enabled = True,
      Invite = "cdjEJP3zXC", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD
      RememberJoins = true -- Set this to false to make them join the discord every time they load it up
   },
   KeySystem = False, -- Set this to true to use our key system
   KeySettings = {
      Title = "$ Cash hub | Key ",
      Subtitle = "Key In Discord server",
      Note = "Join discord server for key discord.gg/cdjEJP3zXC ‚ö†Ô∏è Please note $ Cash hub will always use the same key",
      FileName = "$ Cash hub Key", -- It is recommended to use something unique as other scripts using Rayfield may overwrite your key file
      SaveKey = true, -- The user's key will be saved, but if you change the key, they will be unable to use your script
      GrabKeyFromSite = true, -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
      Key = {"https://pastebin.com/raw/a8Lt1jaS"} -- List of keys that will be accepted by the system, can be RAW file links (pastebin, github etc) or simple strings ("hello","key22")
   }
})

